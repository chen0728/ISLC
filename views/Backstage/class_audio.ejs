            <thead>
            <tr class="sch">
                <td style="width: 15%;" align="center">
                    <div class="t text-left" >音频编号
                    <span class="sort-ico" style="padding-right:98px;"><i class="icon-caret-up"></i><i class="icon-caret-down"></i></span>
                    </div>
                    <div class="form-group float-none j_bubble">
                        <input class="form-control" type="text" name="seq_q" id="seq_q" placeholder="" maxlength="20">
                    </div>
                </td>
                <td style="width: 15%;" align="center">
                    <div class="t">课堂名<span class="sort-ico" style="padding-right:108px;"><i class="icon-caret-up"></i><i class="icon-caret-down"></i></span></div>
                    <div class="form-group float-none j_bubble">
                        <input class="form-control" type="text" name="name_n" id="name_n" maxlength="20">
                    </div>
                </td>
                <td width="50%" align="center" class="sorting" tabindex="0" aria-controls="product_searchTab" rowspan="1" colspan="1" aria-label="
                创建时间: activate to sort column ascending" style="width: 113px;">
                    <div class="t" style="padding-right: 10px;">录制时间<span class="sort-ico" style="padding-right:78px"><i class="icon-caret-up"></i><i class="icon-caret-down"></i></span></div>
                    <div class="form-group j_bubble j_datebetween">
                        <input type="text" class="form-control input-xs j_datepicker" placeholder="开始" id="search_s">
                        <input type="text" class="form-control mt-1 input-xs j_datepicker" placeholder="结束" id="search_e">
                    </div>
                </td>
                <td style="width: 20%;" align="center">
                    <div class="t">操作</div>
                    <button class="btn btn-success float-none j_bubble" type="button" id="employeeSeatchBut">搜索
                    </button>
                </td>
            </tr>
            </thead>
            <tbody></tbody>
        </table>
        <nav class="pagination-wrap clearfix" style="height: 20px;margin-top:-22px">
        </nav>
    </div>


    <!--编辑字典-->
    <div class="modal modal-noline fade" id="showAudioModal" tabindex="-1" role="dialog" aria-labelledby="addAccountModalLabel" aria-hidden="true">
        <div class="modal-outer">
            <div class="modal-middle">
                <div class="modal-dialog modal-inner w-65 float-none">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal"></button>
                        </div>
                        <div class="modal-body p-1 pt-3 pl-5">
                            <form class="form-horizontal" action="" method="post"   id="add_account_form">
                                <input id="account_id" type="hidden" name="account_id">
                                <div class="form-group ">
                                    <label id="audioAreaLab" class="control-label w-9" style="font-size: 29px;width: 135px;"></label>
                                </div>
                                <div class="form-group ">
                                    <label class="control-label w-9">播放插件</label>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
